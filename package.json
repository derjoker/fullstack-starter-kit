{
  "name": "fullstack-starter-kit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.15.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.11"
  },
  "scripts": {
    "babel-node": "babel-node",
    "server": "babel-node src/server.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "standard": "^10.0.3"
  },
  "standard": {
    "globals": [
      "fetch",
      "URL"
    ]
  },
  "babel": {
    "presets": [
      ["env", {
        "targets": {
          "node": "current"
        }
      }]
    ]
  }
}
