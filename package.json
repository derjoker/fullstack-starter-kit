{
  "name": "fullstack-starter-kit",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "apollo-server-express": "^1.1.0",
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "graphql": "^0.10.5",
    "graphql-tools": "^1.1.0",
    "mongoose": "^4.11.6",
    "react": "^15.6.1",
    "react-apollo": "^1.4.12",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.11"
  },
  "scripts": {
    "babel-node": "babel-node",
    "server": "nodemon --watch src/server src/server --exec babel-node",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "nodemon": "^1.11.0",
    "standard": "^10.0.3"
  },
  "standard": {
    "globals": [
      "fetch",
      "URL"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
